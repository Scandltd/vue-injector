{
  "name": "@scandltd/vue-injector",
  "version": "1.0.2",
  "description": "Dependency Injection for Vue.js 2",
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Stepan Telitsyn",
      "email": "telitsyn@scand.com"
    }
  ],
  "main": "dist/vue-injector.common.js",
  "module": "dist/vue-injector.esm.js",
  "unpkg": "dist/vue-injector.js",
  "jsdelivr": "dist/vue-injector.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Scandltd/vue-injector.git"
  },
  "typings": "types/index.d.ts",
  "files": [
    "dist/*.js",
    "types/*.d.ts"
  ],
  "keywords": [
    "vue",
    "inject",
    "dependency injection",
    "injection",
    "injector",
    "provider",
    "services",
    "service",
    "di"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c build/rollup.dev.config.js",
    "build": "node build/build.js",
    "lint": "eslint src examples",
    "test": "npm run lint && npm run flow && npm run test:unit && npm run test:e2e && npm run test:types",
    "flow": "flow check",
    "test:unit": "karma start test/unit/karma.conf.js",
    "test:cover": "karma start test/unit/karma.cover.conf.js",
    "test:e2e": "node test/e2e/runner.js",
    "test:types": "tsc -p types/test",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "release": "bash build/release.sh"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-beta.51",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow-vue": "^1.0.0",
    "buble": "^0.19.3",
    "chromedriver": "^2.40.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.10",
    "es6-promise": "^4.2.4",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-vue-libs": "^2.1.0",
    "express": "^4.16.2",
    "express-urlrewrite": "^1.2.0",
    "flow-bin": "^0.66.0",
    "jasmine": "2.8.0",
    "jasmine-core": "^3.1.0",
    "json-formatter-js": "^2.2.0",
    "karma": "^2.0.4",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^3.0.0",
    "nightwatch": "^0.9.20",
    "nightwatch-helpers": "^1.0.0",
    "path-to-regexp": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.16",
    "rollup": "^0.56.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-flow-no-whitespace": "^1.0.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.0.0",
    "selenium-server": "^2.53.1",
    "typescript": "^2.7.2",
    "uglify-js": "^3.3.13",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-loader": "^15.2.1",
    "vue-template-compiler": "^2.5.16",
    "vuepress": "^0.10.0",
    "webpack": "^4.9.0",
    "webpack-dev-middleware": "^3.1.3"
  },
  "bugs": {
    "url": "https://github.com/Scandltd/vue-injector/issues"
  },
  "homepage": "https://github.com/Scandltd/vue-injector",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
