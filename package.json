{
  "name": "@scandltd/vue-injector",
  "version": "3.2.7",
  "description": "Dependency Injection for Vue.js 2",
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Stepan Telitsyn",
      "email": "telitsyn@scand.com"
    }
  ],
  "main": "dist/vue-injector.common.js",
  "module": "dist/vue-injector.esm.js",
  "unpkg": "dist/vue-injector.js",
  "jsdelivr": "dist/vue-injector.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Scandltd/vue-injector.git"
  },
  "typings": "src/index.d.ts",
  "files": [
    "dist/*.js",
    "types/*.d.ts",
    "src/**/*.d.ts"
  ],
  "keywords": [
    "vue",
    "typescript",
    "typescript inject",
    "vue inject",
    "vue injector",
    "vue provider",
    "vue services",
    "vue di",
    "di",
    "dependency injection"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c build/rollup.dev.config.js",
    "build": "node build/build.js",
    "lint": "eslint examples src/**/*.ts --ignore-pattern *.d.ts",
    "lint:fix": "eslint examples src/**/*.ts --ignore-pattern *.d.ts --fix",
    "test": "yarn run lint && yarn run test:unit && yarn run test:types",
    "test:unit": "node_modules/.bin/jest",
    "test:cover": "node_modules/.bin/jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:types": "tsc --noEmit -p .",
    "types": "tsc --emitDeclarationOnly -p .",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "release": "bash build/release.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/core-js": "^2.5.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-istanbul": "^4.1.6",
    "buble": "^0.19.3",
    "core-js": "^3.4.2",
    "coveralls": "^3.0.8",
    "cross-spawn": "^5.0.1",
    "css-loader": "3.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.16.2",
    "express-urlrewrite": "^1.2.0",
    "jest": "^24.9.0",
    "json-formatter-js": "^2.2.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.2",
    "uglify-js": "^3.6.9",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.1.0",
    "vue-loader": "^15.7.2",
    "vue-property-decorator": "^8.3.0",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.2.0",
    "vuex": "^3.1.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2"
  },
  "peerDependencies": {
    "core-js": "^3.4.2"
  },
  "bugs": {
    "url": "https://github.com/Scandltd/vue-injector/issues"
  },
  "homepage": "https://scand.com/products/vue-injector/",
  "jest": {
    "preset": "ts-jest",
    "testRegex": "/test/unit/.*(\\.test.ts|\\.spec.ts)$",
    "setupFiles": [
      "./src/polyfill.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/node_modules"
    ],
    "coverageDirectory": "test/unit/coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
