{
  "name": "@scandltd/vue-injector",
  "version": "3.3.8",
  "description": "Dependency Injection for Vue.js 2",
  "type": "library",
  "license": "GPL-2.0",
  "authors": [
    {
      "name": "Stepan Telitsyn",
      "email": "telitsyn@scand.com"
    }
  ],
  "main": "dist/vue-injector.js",
  "unpkg": "dist/vue-injector.js",
  "jsdelivr": "dist/vue-injector.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Scandltd/vue-injector.git"
  },
  "types": "src/index.d.ts",
  "files": [
    "dist/*.js",
    "types/*.d.ts",
    "src/**/*.d.ts"
  ],
  "keywords": [
    "vue",
    "typescript",
    "typescript inject",
    "vue inject",
    "vue injector",
    "vue provider",
    "vue services",
    "vue di",
    "di",
    "dependency injection"
  ],
  "scripts": {
    "dev": "node examples/server.js",
    "dev:dist": "rollup -wm -c build/rollup.dev.config.js",
    "build": "node build/build.js",
    "lint": "eslint examples src/**/*.ts --ignore-pattern *.d.ts",
    "lint:fix": "eslint examples src/**/*.ts --ignore-pattern *.d.ts --fix",
    "test": "yarn run lint && yarn run test:unit && yarn run test:types",
    "test:unit": "node_modules/.bin/jest --silent",
    "test:cover": "node_modules/.bin/jest --coverage --coverageReporters=text-lcov | coveralls",
    "test:types": "tsc --noEmit -p .",
    "types": "tsc --emitDeclarationOnly -p .",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "release": "bash build/release.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/core-js": "^2.5.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@vue/test-utils": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.0.0",
    "buble": "^0.20.0",
    "core-js": "^3.12.1",
    "coveralls": "^3.1.0",
    "cross-spawn": "^7.0.3",
    "css-loader": "5.2.5",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "express": "^4.17.1",
    "express-urlrewrite": "^1.4.0",
    "jest": "^26.6.3",
    "json-formatter-js": "^2.3.4",
    "terser-webpack-plugin": "^5.1.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.1",
    "typescript": "^4.2.4",
    "uglify-js": "^3.13.7",
    "vue": "^3.0.11",
    "vue-class-component": "^7.2.6",
    "vue-loader": "^15.9.7",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.8.2",
    "vuex": "^4.0.1",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^4.3.0"
  },
  "peerDependencies": {
    "vue": "^3.0.11"
  },
  "bugs": {
    "url": "https://github.com/Scandltd/vue-injector/issues"
  },
  "homepage": "https://scand.com/products/vue-injector/",
  "jest": {
    "preset": "ts-jest",
    "testRegex": "/test/unit/.*(\\.test.ts|\\.spec.ts)$",
    "setupFiles": [
      "./src/polyfill.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/node_modules"
    ],
    "coverageDirectory": "test/unit/coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "all": true,
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
